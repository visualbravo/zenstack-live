{
  "name": "@newproject/analytics",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsdown",
    "watch": "tsdown --watch",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix --fix-suggestions",
    "format:fix": "oxfmt",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@newproject/env": "workspace:*",
    "@umami/node": "0.4.0"
  },
  "devDependencies": {
    "@newproject/config": "workspace:*"
  },
  "packageManager": "bun@1.3.5",
  "exports": {
    ".": {
      "@newproject/source": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts"
